<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>All Students</title>
    <style>
        body {
            margin: 0 auto;
            max-width: 1200px;
            /* background-color: #02e7ff; */
        }

        table {
            margin: 0 auto;
        }

        table,
        th,
        td {
            border: 1px solid black;
            background-color: rgb(231, 245, 250);
            border-collapse: collapse;
        }

        td {
            padding: 4px;
        }

        div {
            display: flex;
            justify-content: center;
        }

        h1,h2 {
            text-align: center;
        }

        h3 {
            text-align: right;
        }

        #pageCnt {
            text-align: center;
            width: 70px;
            font-size: 30px;
        }

        .pagination {
            margin: 12px;
            /* border: 1px solid red; */
        }

        .start,
        .end {
            padding: 12px;
            margin: 0 12px;
        }

        .leftBtn,
        .rightBtn {
            padding: 12px;
        }
    </style>
</head>

<body>

    <h1>Rushikesh Falak - 24DEV059</h1>
    <a href="/logout">Logout</a>
            </h2>
                <div>

                    <table id="tableId">
                        <% for(let val of data) {%>
                            <%const fieldKeys=Object.keys(val); %>
                                <tr>

                                    <%for(var i=0;i<fieldKeys.length; i++) {%>
                                        <th>
                                            <%=fieldKeys[i]%>
                                        </th>
                                        <% } %>

                                </tr>
                                <% break; %>
                                <% } %>
                        <% for(let key of data) {%>
                            <%const fieldValues=Object.values(key); %>
                                <tr>

                                    <%for(var i=0;i<fieldValues.length; i++) {%>
                                        <td>
                                            <a href="<%=fieldValues[i]%>"><%=fieldValues[i]%></a>
                                            
                                        </td>
                                        <% } %>

                                </tr>
                                <% } %>
                    </table>

                </div>  

</body>

</html>